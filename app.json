{
  "expo": {
    "name": "MediMeter",
    "slug": "MediMeter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "..frontend/assets/logo.png",
    "userInterfaceStyle": "light",
    "permissions": ["CAMERA", "CAMERA_ROLL"],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],

    "ios": {
      "supportsTablet": true,
      "notification": {
        "icon": "..frontend/assets/logo.png",
        "color": "teal",
        "sound": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
    
  }
}
